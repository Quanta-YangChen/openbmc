[Unit]
Description=Phosphor BMC Console SSH socket for MTIA Blade {blade_num}
Wants=obmc-console@ttyS0.service

[Service]
ExecStartPre=/usr/bin/select-uart-mux {blade_num}
ExecStart=/usr/sbin/sshd -i -h /etc/ssh/ssh_host_rsa_key -o "ForceCommand=/usr/bin/obmc-console-client -i mtia-blade"
SyslogIdentifier=console-client-ttyS0-mtia-blade{blade_num}
ExecReload=/bin/kill -HUP $MAINPID
StandardInput=socket
KillMode=process
